{
    "name": "bref0/composer",
    "homepage": "https://bref0.github.io/composer",
    
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.cloud.tencent.com/composer/"
        }
    ],
    
    "require": {
         "laravel/framework": "9.*"
    },

    "require-all": false,
    "providers": true,
    "output-dir": "./public",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}