{
    "name": "bref0/composer",
    "homepage": "https://bref0.github.io/composer",
    
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    ],
    
    "require": {
         "laravel/framework": "9.*"
    },
    
    "archive": {
        "directory": "dist",
        "format": "zip",
        "prefix-url": "https://composer.fedax.cn/dist"
    },

    "require-all": false,
    "providers": true,
    "output-dir": "./public",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}