{
    "name": "bref0/composer",
    "homepage": "https://bref0.github.io/composer",
    
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/symfony/symfony" 
        }
    ],
    
    "require": {
         "symfony/symfony": "*"
    },
    
    "archive": {
        "directory": "dist",
        "format": "zip",
        "prefix-url": "https://composer.fedax.cn/dist"
    },

    "require-all": false,
    "providers": true,
    "output-dir": "./public"   
}