{
    "name": "Fedax自建composer仓",
    "homepage": "https://composer.fedax.cn",
    
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    ],
    
    "require": {
        "hyperf/framework": "~3.1.0"
    },
    
    "archive": {
        "directory": "dist",
        "format": "zip",
        "prefix-url": "https://composer.fedax.cn/dist"
    },

    "require-all": false,
    "providers": true,
    "output-dir": "./public"   
}