{
    "name": "bref0/composer",
    "homepage": "https://bref0.github.io/composer",

    "repositories": [
         {
            "type": "git",
            "url": "git@codeup.aliyun.com:fedax/sharegym/framework.git",
            "options": {
                "symlink": false,
                "mirror-source": true
            }
         }
    ],

    "require": {
         "gym/framework": "*"
    },
    "require-all": false,
    "require-dependencies": false,
    "require-dev-dependencies": false,
    "providers": true,
    "output-dir": "./public",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "archive": {
        "directory": "dist",
        "format": "tar",
        "skip-dev": true
    }
}
