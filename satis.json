{
    "name": "bref0/composer",
    "homepage": "https://bref0.github.io/composer",
    
    "repositories": [
         {
            "type": "git",
            "url": "git@codeup.aliyun.com:yinlisports/composer/core.git"
         }
    ],
    
    "require": {
         "ingym/core": "*"
    },
    "require-all": false,
    "require-dependencies": false,
    "require-dev-dependencies": false,
    "providers": true,
    "output-dir": "./public",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "archive": {
        "directory": "dist",
        "format": "zip"
    }
}